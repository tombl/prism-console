{
  "name": "prism-console",
  "version": "1.1.0",
  "description": "Prism but outputs to the browser console",
  "keywords": [
    "browser",
    "console",
    "highlight",
    "prism"
  ],
  "homepage": "github.com/tomblcode/prism-console",
  "repository": {
    "type": "git",
    "url": "github.com/tomblcode/prism-console.git"
  },
  "license": "MIT",
  "author": "tomblcode",
  "files": [
    "README.md",
    "cjs/index.js",
    "cjs/index.d.ts",
    "cjs/theme.d.ts",
    "esm/index.js",
    "esm/index.d.ts",
    "esm/theme.d.ts"
  ],
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "build": "rollup -c --environment MINIFY",
    "test": "nyc ava",
    "watch": "rollup -cw"
  },
  "dependencies": {},
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@types/css": "0.0.31",
    "@types/prismjs": "^1.16.0",
    "ava": "^2.4.0",
    "css": "^2.2.4",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "prismjs": "^1.17.1",
    "rollup": "^1.27.14",
    "rollup-plugin-shim": "^1.0.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "prismjs": "^1.17.1"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**/*.ts"
    ]
  }
}
